{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "valo-accounts",
  "version": "0.1.0",
  "identifier": "net.noma99t.valo-accounts",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ValoAccounts",
        "width": 700,
        "height": 310,
        "minWidth": 510,
        "minHeight": 310,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "app",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "nsis": null,
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    }
  }
}
